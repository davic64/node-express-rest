{
  "name": "node-express-rest",
  "version": "1.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "dev": "ts-node-dev src/index.ts",
    "build": "cross-env NODE_ENV=production tsc",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml --env-file .env.dev up --build -d",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml --env-file .env.prod up --build -d"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.1",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.3",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "cross-env": "^7.0.3",
    "prisma": "^6.6.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "winston": "^3.17.0",
    "zod": "^3.24.3"
  }
}
